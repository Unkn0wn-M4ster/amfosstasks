{
    "manifest_version": 3,
    "name": "weather app",
    "description": "Displays weather",
    "version": "0.0.1",
    "action": {
        "default_popup": "./index.html",
        "default_icon": {
            "16": "./icon_16.png",
            "48": "./icon_48.png",
            "128": "./icon_128.png"
        }
    },
    "icons": {
        "16": "./icon_16.png",
        "48": "./icon_48.png",
        "128": "./icon_128.png"
    },
    "permissions": [
        "activeTab"
    ]
}
